{
  "name": "metalsmith-sitemap",
  "version": "1.2.2",
  "description": "A metalsmith plugin for generating a sitemap.xml file.",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "files": [
    "lib"
  ],
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib test/fixtures/**/build",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "build": "rollup -c",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ExtraHop/metalsmith-sitemap.git"
  },
  "keywords": [
    "metalsmith",
    "sitemap"
  ],
  "author": {
    "name": "ExtraHop Networks",
    "url": "http://www.extrahop.com"
  },
  "contributors": [
    {
      "name": "Aaron Boushley"
    },
    {
      "name": "Nick Braun"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ExtraHop/metalsmith-sitemap/issues"
  },
  "homepage": "https://github.com/ExtraHop/metalsmith-sitemap",
  "jest": {
    "transform": {
      ".(ts|tsx|js)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {
    "is": "^3.0.1",
    "lodash.get": "^3.7.0",
    "lodash.identity": "^3.0.0",
    "lodash.pick": "^3.1.0",
    "multimatch": "^2.0.0",
    "sitemap": "^1.1.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "assert-dir-equal": "^1.0.1",
    "jest": "^23.6.0",
    "metalsmith": "^2.3.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.65.2",
    "rollup-plugin-typescript2": "^0.17.0",
    "ts-jest": "^23.10.3",
    "typescript": "^3.0.3"
  }
}
